{
  "name": "orume-api-node",
  "version": "1.0.2",
  "description": "API Wrapper for Orume API",
  "main": "dist/index.js",
  "scripts": {
    "test": "node ./tests/index.js",
    "build": "tsc --build tsconfig.json",
    "prepare": "husky install",
    "prettify": "npm run lint && npm run format",
    "lint": "eslint src/**/*.ts --fix",
    "watch": "nodemon --watch server --inspect ./index.js",
    "format": "prettier --config .prettierrc src/**/*.ts --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Orume-Studios/Orume-API-Wrapper.git"
  },
  "keywords": [
    "orume-api",
    "orume-api-wrapper",
    "orume-api-nodejs",
    "license-api"
  ],
  "author": {
    "email": "support@orume.id",
    "name": "Orume Studios",
    "url": "https://orume.id"
  },
  "authors": [
    "Yusta"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Orume-Studios/Orume-API-Wrapper/issues"
  },
  "homepage": "https://github.com/Orume-Studios/Orume-API-Wrapper#readme",
  "dependencies": {
    "node-fetch": "2.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "typescript": "^4.6.3"
  }
}
